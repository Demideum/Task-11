namespace IJunior_Task11
{
    internal class Program
    {
        static void Main(string[] args)
        {           
            int drugStartedDay = 5;
            int drugNumberDays = 96;
            int drugFrequencyDays = 7;

            for (int i = drugStartedDay; i <= drugNumberDays; i += drugFrequencyDays)
            {
                Console.WriteLine(i);
            }
        }
    }
}
